pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "Build step"'
      }
    }
    stage('Test') {
      steps {
        sh 'echo "Test step"'
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo "Deploy step"'
      }
    }
    stage('Deploy 2') {
      steps {
        sh 'echo "Deploy step"'
      }
    }
    stage('Deploy 3') {
      steps {
        sh 'echo "Deploy step"'
      }
    }
    stage('Blue Ocean Deploy 4') {
      steps {
        echo 'update Jenkinsfile from blue ocean'
      }
    }
  }
}